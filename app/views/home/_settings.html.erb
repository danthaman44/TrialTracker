<div class="tab-pane fade" id="settings">

  <div class = "row-fluid">

      <fieldset>
    <legend><%= @first_trial.trialName %> &nbsp <small>Last updated on <%= @first_trial.updated_at.strftime("%m/%d/%Y") %></small> </legend>

<%= form_for @first_trial, :html=>{ :class=>"form-horizontal"} do |trial|  %>
 <form class="form-horizontal">

  <div class="control-group">
    <label class="control-label" for="inputEmail">Trial Name</label>
    <div class="controls">
       <%= trial.text_field :trialName, :type => 'text', :style=>"height:30px"%>
    </div>
  </div>

  <div class="control-group">
    <label class="control-label" for="inputPassword">Trial ID</label>
    <div class="controls">
       <%= trial.text_field :trialID, :type => 'text', :style=>"height:30px"%>
    </div>
  </div>

  <div class="control-group">
    <label class="control-label" for="inputPassword">End Date</label>
    <div class="controls">
       <%= trial.text_field :endDate, :type => 'text', :style=>"height:30px"%>
    </div>
  </div>

  <div class="control-group">
    <label class="control-label" for="inputEmail">Enrolled Goal</label>
    <div class="controls">
      <%= trial.text_field :enrolledGoal, :type => 'text', :style=>"height:30px"%>
      <!--<input type="text" id="inputEmail" value='<%= @first_trial.enrolledGoal %>' style="height:30px">-->
    </div>
  </div>


  <div class="control-group">
    <label class="control-label" for="inputPassword">Completed Goal</label>
    <div class="controls">
      <%= trial.text_field :completedGoal, :type => 'text', :style=>"height:30px"%>
    </div>
  </div>

 <!-- <div class="control-group">
    <label class="control-label" for="inputPassword">End Date</label>
    <div class="controls">

<%= trial.text_field :endDate, :type => 'text', :style=>"height:30px", :id=>"datepicker" %>
    	 :value => @first_trial.endDate.strftime("%m/%d/%Y") logger.info(@first_trial.enrolledGoal)<input type="text" value='<%= @first_trial.endDate %>' id="datepicker" data-date-format="mm-dd-yyyy" style="height:30px"> 

    </div>
  </div> -->


  <div class="control-group">
    <div class="controls">
      <table>
      <tr>
        <td>
          <%= trial.submit "Update", :class=>"btn btn-success"%> 

        </td>
        <td>
          &nbsp &nbsp
        </td>

      </tr>
    </table>

  
</div>



</div>
</fieldset>
    <% end %>


  </div>

  <div class = "row-fluid">
    <div class="span12">


  
</form>

    </div> <!-- end span12 -->
  </div>

</div><!-- end settings -->
</div><!-- end settings -->