	
 <div class="tab-pane fade" id="settings">
  <h3 align = "center"> Edit your Trial Settings </h3>
  <div> &nbsp </div>

  <div class = "row-fluid">
		<div class="span12">




 <form class="form-horizontal">
  <%= form_for(@first_trial) do |f| %>
  <div class="control-group">
    <label class="control-label" for="inputPassword">Trial Name</label>
    <div class="controls">

      <input type="text" value='<%= @first_trial.trialName %>' id="datepicker" data-date-format="mm-dd-yyyy" style="height:30px">
      
    </div>
  </div>

  <div class="control-group">
    <label class="control-label" for="inputPassword">Trial ID</label>
    <div class="controls">
      <input type="text" value='<%= @first_trial.trialID %>' id="datepicker" data-date-format="mm-dd-yyyy" style="height:30px">
      
    </div>
  </div>


  <div class="control-group">
    <label class="control-label" for="inputEmail">Enrolled Goal</label>
    <div class="controls">
      <input type="text" id="inputEmail" value='<%= @first_trial.enrolledGoal %>' style="height:30px">
    </div>
  </div>

  <div class="control-group">
    <label class="control-label" for="inputPassword">Completed Goal</label>
    <div class="controls">

      <input type="text" id="inputPassword" value='<%= @first_trial.completedGoal %>' style="height:30px">
    </div>
  </div>

  <div class="control-group">
    <label class="control-label" for="inputPassword">End Date</label>
    <div class="controls">

    	<input type="text" value='<%= @first_trial.endDate %>' id="datepicker" data-date-format="mm-dd-yyyy" style="height:30px">
      
    </div>
  </div>

 <div class="actions">
    <%= button_to "Update", @first_trial, :method=>:update %>
    <%= button_to "Delete", @trial, :method=>:delete,  :class=>:destroy %>
  </div>
<% end %>



  

</form>

		</div> <!-- end span12 -->
	</div>

</div><!-- end settings -->